<div id="updateable">
    <% var reviewNum = 0;%>

    <% reviews.forEach(function(review){ %>

        <% reviewNum += 1; %>

        <div class="container-fluid">

            <div class="row reviews s-font">
                <div class="review-thumbnail-wrapper">
                    <a href="/profile-<%= review.username %>">
                        <img class="review-thumbnail" style="background: url('images/restTest1.jpg') 50% 50%; background-size: cover;">
                    </a>
                </div>

                <div class="review-wrapper">
                    <div class ="row">
                        <div class="review-header userLink"><a href="/profile?username=<%= review.username %>"><%= review.username %></a></div>
                        <% var divID = "review-date" + reviewNum.toString();%>
                        <div class="review-date align-right" id='<%=divID%>'><%= review.date %></div>
                        <script>formatDate(<%= review.date.getDate() %>,<%= review.date.getMonth() %>,<%= review.date.getFullYear() %>,"<%=divID%>");</script>
                    </div>

                    <div class ="row">
                        <% divID = "review-star" + reviewNum.toString();%>
                        <div class="col-12" id='<%=divID%>'><%= review.rating %> out of 7 Chips</div>
                        <script>getStarRating(<%= review.rating %>, "<%=divID%>");</script>
                    </div>

                    <div class ="row">
                        <div class="col-12"><%= review.review %></div>
                    </div>

                </div>
            </div>

        </div>

        <hr>

    <% }); %>

</div>



